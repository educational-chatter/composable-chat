<resources>
    <string name="app_name">ComposableChat</string>


    <!--    Language-->
    <string name="default_language">ru</string>
    <string name="default_country">RU</string>

    <!--    Patterns phone-->
    <string-array name="phone_patterns">
        <item>\\+[0–9]{13}</item>
        <item>\\+[0–9]{12}</item>
        <item>\\+[0–9]{11}</item>
        <item>\\+[0–9]{9}</item>
        <item>[0–9]{11}</item>
        <item>\\+7 \\([0–9]{3}\\) [0–9]{3}–[0–9]{2}–[0–9]{2}</item>
        <item>\\+7\\([0–9]{3}\\)–[0–9]{3}–[0–9]{2}–[0–9]{2}</item>
        <item>\\+7 [0–9]{3} [0–9]{3} [0–9]{2} [0–9]{2}</item>
        <item>\\+7 [0–9]{3} [0–9]{3}–[0–9]{2}–[0–9]{2}</item>
        <item>[0–9] [0–9]{3} [0–9]{3}–[0–9]{2}–[0–9]{2}</item>
    </string-array>

    <string name="salt">C~kW]cq76(a?m[UbJ)drw+Wh6>W[3Wsj</string>

    <string name="urlChatScheme">https</string>
    <string name="urlChatHost">release.craft-talk.com</string>
    <string name="urlChatNameSpace">webchat_ASDK</string>
    <string name="chat_file_provider_authorities">com.crafttalk.chat.fileprovider</string>

    <string name="default_notification_channel_id">0</string>

    <string name="chat_avatar">Chat avatar</string>

</resources>